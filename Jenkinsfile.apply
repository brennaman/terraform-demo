pipeline {
  environment {
    registry = "brennaman3/terraform-rollout-plan"
    registryCredential = 'dockerhub'
    dockerImage = ''
    TERRAFORM_CMD = 'docker run -w /data -v "$(pwd)":/data'
  }
  agent any
  stages {
    stage("Docker Pull") {
      steps{
        sh "docker pull $DOCKER_TRIGGER_REPO_NAME"
      }
    }
  }
}